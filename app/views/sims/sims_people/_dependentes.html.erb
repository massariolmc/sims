
<table class="table table-condensed">
  <thead>
    <tr>
      <th><%= t('sims.sims_dependents.fields.pesdepnome') %></th>
      <th><%= t('sims.sims_dependents.fields.pesdepgrau') %></th>
      <th><%= t('sims.sims_dependents.fields.pesdepsexo') %></th>
      <th><%= t('sims.sims_dependents.fields.pesdepdn') %></th>
      <th><%= t('sims.sims_dependents.fields.pesdeppesid') %></th>
      <th><%= t('sims.sims_dependents.fields.pes_cracha_id') %></th>            
    </tr>
  </thead>

  <tbody>
    <% @sims_dependents.each do |sims_dependent| %>
      <tr>
        <td><%= sims_dependent.pesdepnome %></td>
        <td><%= sims_dependent.family(sims_dependent.pesdepgrau) %></td>
        <td><%= sims_dependent.pesdepsexo %></td>
        <td><%= sims_dependent.pesdepdn.strftime("%d/%m/%Y") %></td>
        <td><%= sims_dependent.sims_person_id(sims_dependent.pesdeppesid) %></td>
        <td><%= sims_dependent.pegar_cracha(sims_dependent.pesdepid) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
