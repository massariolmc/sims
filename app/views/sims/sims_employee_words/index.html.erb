<p id="notice"><%= notice %></p>

<h1>Sims Employee Words</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sims_employee_words.each do |sims_employee_word| %>
      <tr>
        <td><%= link_to 'Show', sims_employee_word %></td>
        <td><%= link_to 'Edit', edit_sims_employee_word_path(sims_employee_word) %></td>
        <td><%= link_to 'Destroy', sims_employee_word, method: :delete, data: { confirm: 'Are you sure?' } %></td>
